def isSorted(arr, n):  
    for i in range(n): 
        for j in range(i + 1, n): 
            if arr[j] < arr[i]: 
                return False   # Correct indentation
    return True 

if __name__ == "__main__": 
    arr = [1, 2, 3, 4, 5] 
    n = len(arr) 
    ans = isSorted(arr, n) 
    if ans: 
        print("True") 
    else: 
        print("False")
